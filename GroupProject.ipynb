    "print(\"----------------------------------\")\n",
    "\n",
    "# TASK: Predict the outcome on the testing set and store it in a variable\n",
    "# named y_predicted\n",
    "y_predicted = grid_search.predict(x_test)\n",
    "\n",
    "# Print the classification report\n",
    "print(\"Classification Report\")\n",
